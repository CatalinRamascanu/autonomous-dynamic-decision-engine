{
  "input-data":[
    {
      "input-id":"adobeInput",
      "data-types":{
        "auth_rate":"float",
        "num_online_users":"int",
        "wrong_pass_rate":"float",
        "pass-status":"string"
      }
    }
  ],
  "rules":[
    {
      "rule-id":"rule_01",
      "actors":["auth_rate","pass-status"],
      "condition": "auth_rate > 1000",
      "actions":["auth-rate-action"]
    },
    {
      "rule-id":"rule_02",
      "actors":["num_online_users", "auth_rate", "wrong_pass_rate"],
      "condition": "wrong_pass_rate = auth_rate",
      "actions":["auth-rate-action"]
    }
  ]
}